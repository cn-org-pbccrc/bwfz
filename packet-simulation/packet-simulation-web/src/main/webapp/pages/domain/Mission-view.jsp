<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<form  class="form-horizontal">
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务名称:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="nameID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务负责人:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="directorID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务开始时间:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="startDateID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务结束时间:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="endDateID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务状态:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="statusID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务描述:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="descriptionID"></p>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3 control-label">任务建立人:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="taskCreatorID"></p>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3 control-label">任务建立时间:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="taskCreatedTimeID"></p>
                    </div>
                </div>
</form>