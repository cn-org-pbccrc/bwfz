<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<form  class="form-horizontal">
	           <div class="form-group">
                    <label class="col-lg-3 control-label">任务名称:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="taskNameID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">发送渠道:</label>
                    <div class="col-lg-9">
                           <p class="form-control-static" id="sendChannelID"></p>
                    </div>
                </div>
	           <div class="form-group">
                    <label class="col-lg-3 control-label">定时发送:</label>
                    <div class="col-lg-9">
